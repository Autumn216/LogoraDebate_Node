{
    "name": "logora-debate",
    "version": "1.8.0",
    "description": "Logora Debate Plugin",
    "main": "index.js",
    "scripts": {
        "test": "bit test --all",
        "test:coverage": "bit test --coverage",
        "test:server": "mocha --reporter spec server/specs/status",
        "dev:client": "webpack --progress --color --watch --config webpack.config.js --mode=development --env TARGET_ENV=development",
        "dev:server": "webpack --progress --color --watch --config webpack.server.js --mode=development --env TARGET_ENV=development",
        "staging:client": "webpack --progress --mode=production --config webpack.config.js --env TARGET_ENV=staging",
        "staging:server": "webpack --progress --mode=production --config webpack.server.js --env TARGET_ENV=staging",
        "prod:client": "webpack --progress --mode=production --config webpack.config.js --env TARGET_ENV=production",
        "prod:server": "webpack --progress --mode=production --config webpack.server.js --env TARGET_ENV=production",
        "build:client": "webpack --progress --mode=production --config webpack.config.js",
        "build:server": "webpack --progress --mode=production --config webpack.server.js",
        "prod:client:link": "bit link && webpack --progress --mode=production --config webpack.config.js --env TARGET_ENV=production",
        "start-dev": "webpack serve --mode=development --env TARGET_ENV=development",
        "start": "pm2-runtime ./dist/init.js --max-memory-restart 120M --name logora-render -i 4",
        "monitor": "pm2 monit"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hboisgibault/LogoraDebate.git"
    },
    "engines": {
        "node": ">=16.0"
    },
